<div class="modal fade" id="notificationsModal" role="dialog">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal">&times;</button>
                <h4 class="modal-title">Tất cả thông báo</h4>
            </div>
            <div class="modal-body">
                <div class="mark-as-readed">
                    <a href="#" id="popup_mark_as_read_modal"><strong>Đánh dấu là đã đọc</strong></a>
                </div>
                <hr style="width: 90%; margin: auto;">
                <ul class="list-notifications">
                    <% notifications.forEach(function(notification) { %>
                        <li>
                            <%- notification %>
                        </li>
                    <% }) %>
                </ul>
                <hr style="width: 90%; margin: auto;">
                <div class="read-more-notif">
                    <a href="#" id="link-read-more-notif"><strong>Xem thêm 10 thông báo</strong></a>
                </div>
            </div>
        </div>
    </div>
</div>